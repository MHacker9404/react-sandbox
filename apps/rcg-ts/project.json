{
    "root": "apps/rcg-ts",
    "sourceRoot": "apps/rcg-ts/src",
    "projectType": "application",
    "targets": {
        "build": {
            "executor": "@nrwl/web:webpack",
            "outputs": ["{options.outputPath}"],
            "defaultConfiguration": "production",
            "options": {
                "compiler": "babel",
                "outputPath": "dist/apps/rcg-ts",
                "index": "apps/rcg-ts/src/index.html",
                "baseHref": "/",
                "main": "apps/rcg-ts/src/main.tsx",
                "polyfills": "apps/rcg-ts/src/polyfills.ts",
                "tsConfig": "apps/rcg-ts/tsconfig.app.json",
                "assets": [
                    "apps/rcg-ts/src/favicon.ico",
                    "apps/rcg-ts/src/assets"
                ],
                "styles": ["apps/rcg-ts/src/styles.scss"],
                "scripts": [],
                "webpackConfig": "@nrwl/react/plugins/webpack"
            },
            "configurations": {
                "production": {
                    "fileReplacements": [
                        {
                            "replace": "apps/rcg-ts/src/environments/environment.ts",
                            "with": "apps/rcg-ts/src/environments/environment.prod.ts"
                        }
                    ],
                    "optimization": true,
                    "outputHashing": "all",
                    "sourceMap": false,
                    "namedChunks": false,
                    "extractLicenses": true,
                    "vendorChunk": false
                },
                "dev": {
                    "fileReplacements": [
                        {
                            "replace": "apps/rcg-ts/src/environments/environment.ts",
                            "with": "apps/rcg-ts/src/environments/environment.dev.ts"
                        }
                    ],
                    "optimization": false,
                    "outputHashing": "all",
                    "sourceMap": true,
                    "namedChunks": false,
                    "extractLicenses": false,
                    "vendorChunk": false
                }
            }
        },
        "serve": {
            "executor": "@nrwl/web:dev-server",
            "options": {
                "buildTarget": "rcg-ts:build:dev",
                "hmr": true
            },
            "configurations": {
                "dev": {
                    "buildTarget": "rcg-ts:build:dev",
                    "hmr": false
                },
                "production": {
                    "buildTarget": "rcg-ts:build:production",
                    "hmr": false
                }
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["apps/rcg-ts/**/*.{ts,tsx,js,jsx}"]
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": ["coverage/apps/rcg-ts"],
            "options": {
                "jestConfig": "apps/rcg-ts/jest.config.js",
                "passWithNoTests": true
            }
        }
    },
    "tags": []
}