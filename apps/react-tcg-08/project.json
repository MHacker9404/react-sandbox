{
    "root": "apps/react-tcg-08",
    "sourceRoot": "apps/react-tcg-08/src",
    "projectType": "application",
    "targets": {
        "build": {
            "executor": "@nrwl/web:webpack",
            "outputs": ["{options.outputPath}"],
            "defaultConfiguration": "production",
            "options": {
                "compiler": "babel",
                "outputPath": "dist/apps/react-tcg-08",
                "index": "apps/react-tcg-08/src/public/index.html",
                "baseHref": "/",
                "main": "apps/react-tcg-08/src/main.js",
                "polyfills": "apps/react-tcg-08/src/polyfills.js",
                "tsConfig": "apps/react-tcg-08/tsconfig.app.json",
                "assets": [
                    "apps/react-tcg-08/src/public/favicon.ico",
                    "apps/react-tcg-08/src/assets"
                ],
                "styles": ["apps/react-tcg-08/src/styles.scss"],
                "scripts": [],
                "webpackConfig": "@nrwl/react/plugins/webpack"
            },
            "configurations": {
                "production": {
                    "fileReplacements": [
                        {
                            "replace": "apps/react-tcg-08/src/environments/environment.js",
                            "with": "apps/react-tcg-08/src/environments/environment.prod.js"
                        }
                    ],
                    "optimization": true,
                    "outputHashing": "all",
                    "sourceMap": false,
                    "namedChunks": false,
                    "extractLicenses": true,
                    "vendorChunk": false
                }
            }
        },
        "serve": {
            "executor": "@nrwl/web:dev-server",
            "options": {
                "buildTarget": "react-tcg-08:build",
                "hmr": true
            },
            "configurations": {
                "production": {
                    "buildTarget": "react-tcg-08:build:production",
                    "hmr": false
                }
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["apps/react-tcg-08/**/*.{ts,tsx,js,jsx}"]
            }
        }
    },
    "tags": []
}
